{
  "name": "fisea",
  "version": "0.0.0",
  "description": "A parser like the search part of Github",
  "main": "dist/fisea.umd.js",
  "jsnext:main": "dist/fisea.es.js",
  "scripts": {
    "start": "node dev-server.js",
    "prepublish": "yarn prod",
    "preprod": "mkdir -p dist; NODE_ENV='production' node prod.js",
    "prod": "babili dist/fisea.js --no-comments --minified -o dist/fisea.min.js",
    "_test": "xo && ava",
    "test": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nju33/fisea.git"
  },
  "keywords": [
    "ui",
    "browser",
    "commonjs",
    "es"
  ],
  "author": "nju33",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nju33/fisea/issues"
  },
  "homepage": "https://github.com/nju33/fisea#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "6",
    "babel-helpers": "^6.23.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.23.0",
    "babili": "^0.0.9",
    "browser-sync": "^2.18.2",
    "cssnano": "^3.8.1",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "gulp-postcss": "^6.2.0",
    "nightmare": "^2.8.1",
    "postcss-easings": "^0.3.0",
    "postcss-modules": "^0.5.2",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-svelte": "^1.0.0",
    "xo": "^0.17.1"
  },
  "ava": {
    "files": [
      "test/specs/*.js"
    ]
  },
  "xo": {
    "env": [
      "node",
      "browser"
    ],
    "esnext": true,
    "space": 2,
    "ignores": [
      "es/**"
    ],
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": "test/fixtures/*js",
        "esnext": false,
        "globals": [
          "fisea"
        ]
      }
    ]
  },
  "dependencies": {
    "redux": "^3.6.0"
  }
}
